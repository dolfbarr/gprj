{
  "name": "gprj",
  "description": "Clean & simple CLI git multi project manager",
  "version": "0.1.1",
  "author": "Dolf Barr @dolfbarr",
  "bin": {
    "gprj": "./bin/run"
  },
  "bugs": "https://github.com/dolfbarr/gprj/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/lodash": "4.14.171",
    "@types/lowdb": "1.0.9",
    "chalk": "4.1.1",
    "figures": "3.2.0",
    "lodash": "4.17.21",
    "lowdb": "1.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "cross-env": "^7.0.3",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dolfbarr/gprj",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "gprj",
    "hooks": {
      "init": "./lib/hooks/init/db"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "dolfbarr/gprj",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "cross-env NODE_ENV=test nyc --reporter text --extension .ts mocha --forbid-only \"test/**/*.test.ts\" -R list && rimraf test/gprj_db.json",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "pre-commit": [
    "test"
  ]
}
